Algoritmo sin_titulo
	//DECLARACION DE VARIABLES
	DEFINIR i Como Entero
	DEFINIR j COMO ENTERO 
	DEFINIR m Como Entero
	DEFINIR n Como Entero
	
	//INICIALIZACION DE VARIABLES
	
	m <- 9
	n <- 9
	//PROCESO
	para  i <-1 hasta m 
		 
		ESCRIBIR 'TABLA DE MULTIPLICAR DEL ', i
		para j<- 1 hasta n 
			ESCRIBIR i, 'X', j, ' = ',i*j
			
		fin para
		
		ESCRIBIR ''
	fin para
FinAlgoritmo
